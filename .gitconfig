[branch]
	sort = -committerdate
[column]
	ui = auto
[diff]
	algorithm = histogram
	colorMoved = plain
	mnemonicPrefix = true
	renames = true
[fetch]
	all = true
[init]
	defaultBranch = main
[merge]
	conflictstyle = zdiff3
[push]
	autoSetupRemote = true
	followTags = true
[tag]
	sort = version:refname
